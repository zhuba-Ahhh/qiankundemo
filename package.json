{
  "name": "qiankundemo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "install": "npm-run-all --serial install:*",
    "install:base": "cd base && pnpm i",
    "install:vue": "cd vue-ts && pnpm i",
    "install:vue-vite": "cd vue-vite && pnpm i",
    "install:react": "cd react-ts && pnpm i",
    "install:react-vite": "cd react-vite && pnpm i",
    "serve": "npm-run-all --parallel serve:*",
    "serve:react": "cd react-ts && pnpm start",
    "serve:react-vite": "cd react-vite && pnpm dev",
    "serve:vue": "cd vue-ts && pnpm serve",
    "serve:vue-vite": "cd vue-vite && pnpm dev",
    "serve:base": "cd base && pnpm start",
    "build": "pnpm clear && npm-run-all build:* && bash ./scripts/bundle.sh",
    "build:react": "cd react-ts && pnpm build",
    "build:react-vite": "cd react-vite && pnpm build",
    "build:vue": "cd vue-ts && pnpm build",
    "build:vue-vite": "cd vue-vite && pnpm build",
    "build:base": "cd base && pnpm build",
    "clear": "npm-run-all clear:*",
    "clear:base": "cd base && rm -rf build",
    "clear:react": "cd react-ts && rm -rf build",
    "clear:react-vite": "cd react-vite && rm -rf dist",
    "clear:vue-vite": "cd vue-vite && rm -rf dist",
    "clean": "find . -name 'node_modules' -type d -exec rm -rf '{}' +"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
